{
  "headlessBrowser": "false",
  "inputDelay": 100,
  "loginUrl": "https://www.instagram.com",
  "loginInputSelector": "input[name='username']",
  "passwordInputSelector": "input[name='password']",
  "loginButtonSelector": "button[type='submit']",
  "neededTabsToSearch": 3,
  "searchBarSelector": ".thisIsSearchBar",
  "searchResultXpath":  "//span[normalize-space(.)='#hashtagWord']/ancestor::span/following-sibling::span/span/span/span",

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }

    }
  }
}